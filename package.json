{
    "name": "super-swank-featured-image",
    "version": "1.0.0",
    "description": "WordPress plugin for setting default featured images",
    "scripts": {
        "build": "wp-scripts build src/block-editor/index.js --output-path=build/block-editor",
        "start": "wp-scripts start src/block-editor/index.js --output-path=build/block-editor",
        "packages-update": "wp-scripts packages-update",
        "plugin-zip": "wp-scripts plugin-zip"
    },
    "devDependencies": {
        "@wordpress/scripts": "^30.7.0"
    },
    "files": [
        "build",
        "languages",
        "super-swank-featured-image.php",
        "readme.txt",
        "LICENSE"
    ],
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/edequalsawesome/super-swank-featured-image.git"
    },
    "keywords": [
        "wordpress",
        "plugin",
        "featured-image"
    ],
    "author": "eD! Thomas",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/edequalsawesome/super-swank-featured-image/issues"
    },
    "homepage": "https://github.com/edequalsawesome/super-swank-featured-image#readme"
}
